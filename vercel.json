{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "status": 308,
      "headers": { "Location": "/docs" }
    },
    {
      "src": "/docs(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/docs/$1"
    }
  ]
}
